{
    "digital_input": 
    {
        "prefix": "digital_input",
        "body": "digital_input ${1:Name};"
    },
    "digital_output": 
    {
        "prefix": "digital_output",
        "body": "digital_output ${1:Name};"
    },
    "analog_input": 
    {
        "prefix": "analog_input",
        "body": "analog_input ${1:Name};"
    },
    "analog_output": 
    {
        "prefix": "analog_output",
        "body": "analog_output ${1:Name};"
    },
    "string_input": 
    {
        "prefix": "string_input",
        "body": "string_input ${1:Name}[${2:stringSize}];"
    },
    "string_output": 
    {
        "prefix": "string_output",
        "body": "string_output ${1:Name};"
    },
    "push": 
    {
        "prefix": "push",
        "body":
        [   
            "push ${1:DigitalInput}",
            "{",
                "   ${2://code here}",
            "}"
        ]
    },
    "release": 
    {
        "prefix": "release",
        "body":
        [ 
            "release ${1:DigitalInput}",
            "{",
                "   ${2://code here}",
            "}"
        ]
    },
    "change": 
    {
        "prefix": "change",
        "body":
        [ 
            "change ${1:StringOrAnalogInput}",
            "{",
                "   ${2://code here}",
            "}"
        ]
    },
    "integer_parameter": 
    {
        "prefix": "integer_parameter",
        "body": "integer_parameter ${1:Name};"
    },
    "string_parameter": 
    {
        "prefix": "string_parameter",
        "body": "string_parameter ${1:Name};"
    },
    "trace": 
    {
        "prefix": "trace",
        "body": "trace(\"${1:string}\",${2:args});"
    },
    "makestring":
    {
        "prefix": "makestring",
        "body": "makestring(${1:destinationVariable},\"${2:string}\",${3:args});"
    },
    "find":
    {
        "prefix": "find",
        "body": "find(\"${1:match_string}\",${2:source_string});"
    },
    "mid":
    {
        "prefix": "mid",
        "body": "mid(${1:source},${2:start},${3:number});"
    },
    "delay": 
    {
        "prefix": "delay",
        "body": "delay(${1:hundredthsOfaSecond});"
    },
    "lower": 
    {
        "prefix": "lower",
        "body": "lower(${1:stringVariable})"
    },
    "upper": 
    {
        "prefix": "upper",
        "body": "upper(${1:stringVariable})"
    },
    "itoa": 
    {
        "prefix": "itoa",
        "body": "itoa(${1:integerVariable})"
    },
    "atoi": 
    {
        "prefix": "atoi",
        "body": "atoi(${1:stringVariable})"
    },
    "chr": 
    {
        "prefix": "chr",
        "body": "chr(${1:integerCode})",
        "description": "Takes the integer value specified and returns the corresponding ASCII character as a one-byte string."
    },
    "pulse": 
    {
        "prefix": "pulse",
        "body": "pulse(${1:hundredthsOfaSecond}, ${2:digitalOutput});"
    },
    "for":
    {
        "prefix": "for",
        "body": 
        [
            "integer ${1:variableName};",
            "",
            "for(${1:variableName} = ${2:value} to ${3:limit})",
            "{",
            "   ${0://code here}",
            "}"
        ]
    },
    "switch":
    {
        "prefix": "switch",
        "body": 
        [
            "switch(${1:expression})",
            "{",
            "   case (${2:value}):",
            "   {",
            "       ${0://code here}",
            "   }",
            "   default:",
            "       trace(\"default\")",
            "}"
        ]
    },
    "if":
    {
        "prefix": "if",
        "body": 
        [
            "if(${1:expression})",
            "{",
            "   ${0://code}",
            "}"
        ]
    },
    "else":
    {
        "prefix": "else",
        "body": 
        [
            "else",
            "{",
            "   ${0://code}",
            "}"
        ]
    },
    "while":
    {
        "prefix": "while",
        "body": 
        [
            "while(${1:expression})",
            "{",
            "   ${0://code}",
            "}"
        ]
    },
    "dountil":
    {
        "prefix": "dountil",
        "body": 
        [
            "do",
            "{",
            "   ${0://code}",
            "}",
            "until(${1:expression});"
        ]
    },
    "function":
    {
        "prefix": "function",
        "body": 
        [
            "function ${1:name}(${2:args})",
            "{",
            "   ${0://code}",
            "}"
        ]
    },
    "string_function":
    {
        "prefix": "string_function",
        "body": 
        [
            "string_function ${1:name}(${2:args})",
            "{",
            "   string ${3:returnValue}[255];",
            "   ${0://code}",
            "   ",
            "   return ($3);",
            "}"
        ]
    },
    "integer_function":
    {
        "prefix": "integer_function",
        "body": 
        [
            "integer_function ${1:name}(${2:args})",
            "{",
            "   integer ${3:returnValue};",
            "   ${0://code}",
            "   ",
            "   return ($3);",
            "}"
        ]
    },
    "GetLastModifiedArrayIndex": {
        "prefix":"getlast",
        "body":
        [
            "GetLastModifiedArrayIndex()"
        ],
        "description": "Returns last modified array index"
    },
    "RegisterDelegate": {
        "prefix":"registerdelegate",
        "body":
        [
            "RegisterDelegate(${1:objectName}, ${2:delegateName}, ${3:callbackName});",
            "",
            "callback function ${3:callbackName}(${4:argsType} args)",
            "{",
            "   ${0://todo: code};",
            "}"
        ],
        "description": "Register Delegates and generate callback function."
    }
}